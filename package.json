    "core-showcase": "node scripts/build/Setup/jfs-showcase.js",
    "jsc-check": "node scripts/build/Setup/jsc-check.js",
    "jsc-generate": "node scripts/build/Setup/jsc-generate.js",
    "set-peer-dependencies": "node scripts/build/Setup/set-peer-dependencies.js",
    "clean-install": "rm -rf ./node_modules ./package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elumeo/jfs-core.git"
  },
