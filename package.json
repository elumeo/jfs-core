{
  "name": "@elumeo/jfs-core",
  "version": "10.0.0",
  "description": "Foundation for all JFS Apps",
  "main": "Main.js",
  "scripts": {
    "start": "webpack serve --config build-tools/webpack/dev-server --port 8080 --host localhost",
    "build": "node scripts/build/Setup/jfs-build.js",
    "build:watch": "node scripts/build/Setup/jfs-build --watch",
    "test": "webpack",
    "prepublish": "cd scripts && npm run build",
    "release": "npm publish --access public",
    "jfs-postinstall": "echo node scripts/build/Setup/postinstall.js",
    "postinstall": "subpkg install && npm run jfs-postinstall",
    "register-scripts": "node scripts/build/Setup/register-scripts.js",
    "check-translations": "node scripts/build/Setup/check-translations.js",
    "deploy-config-files": "node scripts/build/Setup/deploy-config-files.js",
