{
  "name": "@elumeo/jfs-core",
  "version": "9.3.1",
  "description": "Foundation for all JFS Apps",
  "main": "Main.js",
  "scripts": {
    "build": "tsc && node scripts/build/Setup/jfs-prebuild.js",
    "build:watch": "tsc-watch --onCompilationComplete \"node scripts/build/Setup/jfs-prebuild.js\"",
    "jsc-generate": "node ./scripts/build/Setup/jsc-generate.js",
    "deploy-config-files": "node ./scripts/build/Setup/deploy-config-files.js",
    "generate-juwelo-icon-font": "node ./scripts/build/Setup/generate-juwelo-icon-font.js",
    "jsc-check": "node ./scripts/build/Setup/Check/Api.js",
    "prepublish": "cd scripts && npm run build",
    "release": "npm publish --access public",
    "postinstall": "subpkg install",
    "clean-install": "rm -rf ./node_modules ./package-lock.json ./scripts/node_modules ./scripts/package-lock.json && npm i",
    "clean-install-and-rebuild": "rm -rf ./build ./node_modules ./package-lock.json ./scripts/node_modules ./scripts/package-lock.json ./scripts/build && npm i && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elumeo/jfs-core.git"
