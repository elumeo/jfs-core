<+>@use '~react-md/src/scss/react-md';\n@use '../../Style/layout';\n@use '../../Style/colors';\n\n.badges__notifications {\n  &__notification {\n    position: relative;\n    transition-property: transform, max-height, opacity;\n    transition-duration: 300ms;\n\n    &.fadein-enter {\n      max-height: 0;\n      transform: translate3d(layout.$notification-drawer-width, 0, 0);\n    }\n\n    &.fadein-enter.fadein-enter-active {\n      max-height: 500px;\n      transform: translate3d(0, 0, 0);\n    }\n\n    &.fadeout-leave {\n      max-height: 500px;\n      transition-duration: 200ms;\n      transform: translate3d(0, 0, 0);\n    }\n\n    &.fadeout-leave.fadeout-leave-active {\n      max-height: 0;\n      transition-duration: 200ms;\n      transform: translate3d(layout.$notification-drawer-width, 0, 0);\n    }\n\n    &.disappear-leave {\n      transition-duration: 200ms;\n      opacity: 0.9;\n      max-height: 500px;\n    }\n\n    &.disappear-leave.disappear-leave-active {\n      transition-duration: 200ms;\n      opacity: 0;\n      max-height: 0;\n    }\n\n    &.md-card {\n      background: react-md.$md-snackbar-color;\n      color: #fff !important;\n      border-radius: 3px;\n      opacity: 0.9;\n\n      .md-btn {\n        &--icon,\n        &--text {\n          color: #fff !important;\n        }\n      }\n\n      .badge {\n        position: absolute;\n        top: 0;\n        left: 8px;\n\n        .md-badge {\n          background: react-md.$md-snackbar-color;\n\n          &--circular {\n            font-size: 12px;\n          }\n        }\n      }\n\n      .header {\n        padding: 10px 8px 0 8px;\n        display: flex;\n        flex-direction: row;\n        justify-content: space-between;\n        align-items: flex-start;\n        width: 100%;\n      }\n\n      .timestamp {\n        color: rgba(255, 255, 255, .7);\n        font-size: 0.825em;\n        margin-left: auto;\n      }\n    }\n\n    &.error {\n      background: colors.$juw-error-color;\n\n      .badge .md-badge {\n        background: colors.$juw-error-color;\n      }\n    }\n\n    &.success {\n      background: colors.$juw-success-color;\n\n      .badge .md-badge {\n        background: colors.$juw-success-color;\n      }\n    }\n\n    &.clickable {\n      cursor: pointer;\n    }\n\n    .notification-grid {\n      display: flex;\n      flex-direction: row;\n      justify-content: space-between;\n      align-items: flex-start;\n\n      &-actions,\n      &-content {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n      }\n\n      &-content {\n        width: calc(100% - #{react-md.$md-btn-icon-size});\n\n        &:last-child {\n          width: 100%;\n        }\n      }\n    }\n  }\n}\n
